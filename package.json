{
  "name": "@devoinc/genesys-icons",
  "version": "5.0.0",
  "type": "module",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.umd.cjs",
  "module": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "generate": "npm run generate:font && npm run generate:js && npm run generate:react",
    "generate:font": "tsx ./scripts/font/generate.ts",
    "generate:react": "tsx ./scripts/react/generate.ts",
    "generate:js": "tsx ./scripts/js/generate.ts",
    "lint": "oxlint -c oxlintrc.json",
    "build": "vite build",
    "clean": "rm -rf dist",
    "test": "vitest",
    "test:ci": "vitest --coverage",
    "stryker": "npx stryker run",
    "dist": "npm run clean && npm run build && npm run generate",
    "release": "release-it --dry-run",
    "release:ci": "release-it --ci"
  },
  "devDependencies": {
    "@popperjs/core": "^2.11.8",
    "@release-it/conventional-changelog": "^8.0.1",
    "@storybook/addon-a11y": "^8.2.9",
    "@storybook/addon-actions": "^8.2.9",
    "@storybook/addon-docs": "^8.2.9",
    "@storybook/addon-essentials": "^8.2.9",
    "@storybook/addon-links": "^8.2.9",
    "@storybook/node-logger": "^8.2.9",
    "@storybook/react": "^8.2.9",
    "@storybook/react-vite": "^8.2.9",
    "@stryker-mutator/core": "^8.5.0",
    "@stryker-mutator/vitest-runner": "^8.5.0",
    "@types/node": "^22.4.1",
    "@types/react": "^18.3.3",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-v8": "^2.0.5",
    "cheerio": "^1.0.0",
    "decamelize": "^6.0.0",
    "glob-promise": "^6.0.7",
    "happy-dom": "^14.12.3",
    "oxlint": "^0.7.2",
    "pascal-case": "^3.1.2",
    "prettier": "^3.3.3",
    "react-popper": "^2.3.0",
    "react-window": "^1.8.10",
    "release-it": "^17.6.0",
    "sass": "^1.77.8",
    "storybook": "^8.2.9",
    "styled-components": "^6.1.12",
    "svgtofont": "^5.0.3",
    "tsx": "^4.17.0",
    "typescript": "^5.5.4",
    "vite": "^5.4.1",
    "vitest": "^2.0.5"
  },
  "publishConfig": {
    "access": "public"
  },
  "engines": {
    "node": ">=18",
    "npm": ">=7"
  },
  "peerDependencies": {
    "react": ">=16",
    "react-dom": ">=16"
  }
}
